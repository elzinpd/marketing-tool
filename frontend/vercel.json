{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "https://marketing-tool-backend.vercel.app/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 